{
    "manifests": {
        "php-amqplib/rabbitmq-bundle": {
            "manifest": {
                "bundles": {
                    "OldSound\\RabbitMqBundle\\OldSoundRabbitMqBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "etc/": "%ETC_DIR%/",
                    "src/": "%SRC_DIR%/"
                },
                "env": {
