                        "        Payum\\Core\\Model\\Payment:",
                        "            filesystem:",
                        "                storage_dir: '%kernel.root_dir%/Resources/payments'",
                        "                id_property: number",
                        "",
                        "    security:",
                        "        token_storage:",
                        "            Payum\\Core\\Model\\Token:",
                        "                filesystem:",
                        "                    storage_dir: '%kernel.root_dir%/Resources/gateways'",
                        "                    id_property: hash",
                        "",
                        "    gateways:",
                        "        offline:",
                        "            factory: offline",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "72ad834a0f4a99175beddb5e2d049136f4a50a67"
        }
    }
}
