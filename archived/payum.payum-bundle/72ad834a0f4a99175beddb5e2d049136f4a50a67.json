{
    "manifests": {
        "payum/payum-bundle": {
            "manifest": {
                "bundles": {
                    "Payum\\Bundle\\PayumBundle\\PayumBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/payum.yaml": "%CONFIG_DIR%/packages/payum.yaml"
                }
            },
            "files": {
                "config/payum.yaml": {
                    "contents": [
                        "payum:",
