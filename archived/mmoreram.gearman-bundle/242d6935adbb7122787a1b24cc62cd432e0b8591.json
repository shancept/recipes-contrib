{
    "manifests": {
        "mmoreram/gearman-bundle": {
            "manifest": {
                "bundles": {
                    "Mmoreram\\GearmanBundle\\GearmanBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/gearman.yaml": {
                    "contents": [
