                "bundles": {
                    "Kunstmaan\\AdminBundle\\KunstmaanAdminBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/kunstmaan_admin.yaml": {
                    "contents": [
                        "kunstmaan_admin:",
                        "    multi_language: true",
                        "    required_locales: nl|fr|en",
                        "    default_locale: en",
                        "    admin_locales: ['en']",
