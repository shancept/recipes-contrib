{
    "manifests": {
        "qossmic/deptrac-shim": {
            "manifest": {
                "copy-from-recipe": {
                    "deptrac.yaml": "deptrac.yaml"
                },
                "gitignore": [
                    "/.deptrac.cache"
                ]
            },
            "files": {
                "deptrac.yaml": {
                    "contents": [
                        "parameters:",
                        "    paths:",
                        "        - ./src",
                        "    exclude_files:",
                        "        - '#.*test.*#'",
                        "    layers:",
                        "        -",
                        "            name: Controller",
                        "            collectors:",
                        "                -",
                        "                    type: className",
                        "                    regex: .*Controller.*",
                        "        -",
                        "            name: Repository",
                        "            collectors:",
                        "                -",
                        "                    type: className",
                        "                    regex: .*Repository.*",
                        "        -",
                        "            name: Service",
                        "            collectors:",
                        "                -",
                        "                    type: className",
                        "                    regex: .*Service.*",
                        "    ruleset:",
                        "        Controller:",
                        "            - Service",
                        "        Service:",
                        "            - Repository",
                        "        Repository:",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "3c3daab46226c37fc14e10faf81df737a3039d96"
        }
    }
}
